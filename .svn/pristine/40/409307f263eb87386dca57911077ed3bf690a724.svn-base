<%@ taglib prefix="s" uri="/struts-tags"%>

<%@ page import="com.projectwork.constants.TestProjectConstantsIfc"%>

<s:form action="/PersonalProfileUpdateAction" method="post" theme="simple">
	<table width="50%" border="0" cellspacing="3" cellpadding="3" height="100%">
		<tr height="65" />
		<tr>
			<td align="right" class="fieldname"><s:text
					name="profileName.label" />:</td>
			<td align="left"><s:textfield property="profileName" size="20"
					maxlength="20" value="" /></td>
		</tr>
		<tr height="35" />
		<tr>
			<td align="right" class="fieldname"><s:text
					name="profileDesignation.label" />:</td>
			<td align="left"><s:textfield property="profileDesignation"
					size="20" maxlength="20" value="" /></td>
		</tr>
		<tr height="35" />

		<tr>
			<td align="center" colspan="2">
			<button type="submit">
					<s:text name="button.update" />
				</button>
			</td>
		</tr>
		<tr height="15" />
	</table>
</s:form>
<script>
	document.getElementById("settingsTab").style.backgroundColor = "#A9A9F5";
	document.getElementById("personalProfileTab").style.backgroundColor = "#A9A9F5";
</script>
