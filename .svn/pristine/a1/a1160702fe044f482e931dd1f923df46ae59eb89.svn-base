package com.projectwork.bean;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

public class FeederBean implements Serializable
{

    List feederList = new ArrayList();
    private String feeder;
    private int meterIndex;
    private int meterID;
    private int meterVersion;
    private int companyID;
    
    public String getFeeder()
    {
        return feeder;
    }

    public void setFeeder(String feeder)
    {
        this.feeder = feeder;
    }

    public List getFeederList()
    {
        return feederList;
    }

    public void setFeederList(List feederList)
    {
        this.feederList = feederList;
    }

    public int getMeterVersion()
    {
        return meterVersion;
    }

    public void setMeterVersion(int meterVersion)
    {
        this.meterVersion = meterVersion;
    }

    public int getMeterIndex()
    {
        return meterIndex;
    }

    public void setMeterIndex(int meterIndex)
    {
        this.meterIndex = meterIndex;
    }

    public int getCompanyID()
    {
        return companyID;
    }

    public void setCompanyID(int companyID)
    {
        this.companyID = companyID;
    }

    public int getMeterID()
    {
        return meterID;
    }

    public void setMeterID(int meterID)
    {
        this.meterID = meterID;
    }
}
