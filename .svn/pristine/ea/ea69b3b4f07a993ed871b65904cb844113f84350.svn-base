<%@ taglib prefix="s" uri="/struts-tags"%>

<%@ page import="com.projectwork.constants.TestProjectConstantsIfc"%>

<s:form action="/MiscUpdateAction">
	<table width="60%" border="0" cellspacing="3" cellpadding="3" height="100%">
		<tr height="65" />
		<tr>
			<td align="right" class="fieldname"><s:text
					name="meterId.label" />:</td>
			<td align="left"><s:textfield property="meterId" size="20"
					maxlength="20" value="" /></td>
		</tr>
		<tr height="15" />
		<tr>
			<td align="right" class="fieldname"><s:text
					name="feederName.label" />:</td>
			<td align="left"><s:textfield property="feederName"
					size="20" maxlength="20" value="" /></td>
		</tr>
		<tr height="35" />

		<tr>
			<td align="right"><s:submit property="button">
					<s:text name="button.assign" />
				</s:submit></td>
			<td><s:reset>
					<s:text name="button.clear" />
				</s:reset></td>	
		</tr>
		<tr height="15" />
	</table>
</s:form>	
<script>
	document.getElementById("settingsTab").style.backgroundColor = "#A9A9F5";
	document.getElementById("miscTab").style.backgroundColor = "#A9A9F5";
</script>	