<%@ taglib prefix="s" uri="/struts-tags"%>
<%@ taglib prefix="sx" uri="/struts-dojo-tags"%>
<sx:head />

<s:form action="DisableCompanyAction.action" method="post" theme="simple">

	<table width="50%" border="0" cellspacing="3" cellpadding="3"
		height="100%">
		<tr height="65" />
		<tr>
			<td width="50%" align="right" class="fieldname">Select Company to remove:</td>
			<td align="left"><s:select list="companyList" name="companyName"
					id="companyName" listKey="companyName" listValue="companyName" 
					cssStyle="WIDTH:120px"></s:select></td>

			<td width="20%"><button type="submit">
					<s:text name="button.select" />
				</button></td>
				
			<td width="30%"/>
		</tr>
		<tr height="15" />
	</table>
</s:form>
<script>
    document.getElementById("settingsTab").style.textDecoration = "underline";
	document.getElementById("settingsTab").style.backgroundColor = "#9DB1ED";
	document.getElementById("showCompanyTab").style.backgroundColor = "#A9A9F5";
</script>

